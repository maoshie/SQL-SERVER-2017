-- Vari√°vel do tipo Tabela (TABLE); 
-- Declarando uma variavel;

DECLARE @nome_Variavel VARCHAR(10);
SET @nome_Variavel = 'Marcio'
PRINT @nome_Variavel

-- Variavel do tipo tabela;
DECLARE @tabela TABLE (
	cdProduto INT PRIMARY KEY,
	nmProduto VARCHAR(10)
)
INSERT INTO @tabela VALUES (1, 'Sapato')
SELECT * FROM @tabela 
